.report
  &__headerTable
    &__line
      border-top: 1px solid #898989
    &__tittle
      font-weight bold
      font-size 13px
  &__boxSeparator
    padding-top: 10px
    padding-bottom: 10px
  &__separatorProject
    margin 0rem
  &__projectName
    font-size 12px
    color $colorTitleProject
    text-align justify
    margin-bottom 5px
    &__ancla
      display block
      &:hover
        text-decoration none
  &__itemJurado
    font-size 10px
    font-weight bold
    margin-bottom 2px
    margin-top 1px
  &__itemJuradoFechas
    font-size 10px
    font-weight bold
    margin-bottom 2px
    margin-top 1px
    color #1FA4AA
  &__itemJuradoDias
    font-size 12px
    font-weight bold
    margin-bottom 2px
    margin-top 1px
  &__nameDocente
    margin-top 0rem
    margin-bottom 2px
    font-size 13px
  &__correoDocente
    font-size 12px
    margin-top 5px
  &__anclaModal
    text-decoration underline
    cursor pointer
    font-weight bold
    font-size 12px



